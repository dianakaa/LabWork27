<Page x:Class="LabWork27.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LabWork27.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Главная">
    <Grid>
        <Frame x:Name="frame"/>
        <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
            <Button x:Name="Authorization" Content="Авторизация" Click="Authorization_Click"/>
            <Button x:Name="Registration" Content="Регистрация" Click="Registration_Click" />
            <Button x:Name="Setting" Content="Настройки" Click="Setting_Click"/>
        </WrapPanel>
    </Grid>
</Page>
